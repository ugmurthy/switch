Murthys-MacBook-Pro:switch1 murthy$ cat Server.js
'use strict';

const http = require('http');
const url = require('url');
const qs = require('querystring');

// let routes be an object that defines the function for
// each METHOD,PATHNAME pair. this then is invoked by the router
// after using the req.method, baseURI.pathname
let routes = {
	'GET': {
		'/': (req,res) => {
			//res.writeHead(200,{'Content-type':'text/html'});
			//res.end('<h1>Hello Router');
			res.sendFile("/switch.html");
		},
		'/about': (req,res) => {
			res.writeHead(200,{'Content-type':'text/html'});
			res.end('<h1>This is the about page');
			res.sendFile("/switch.html");
		},
		'/switch': (req,res) => {
			// this where one should fetch data from db and respond
			// as JSON
			//res.writeHead(200,{'Content-type':'text/html'});
			//res.end(JSON.stringify(req.queryParams));	
		}
	},
	'POST': {
			// POST request send data in body and in chunks which need assembly
		'/api/login': (req,res) => {
			let body = '';
			req.on('data',data => {
				body +=data;
				// body could be largesize and may need size checks
				console.log(body.length);
				if (body.length > 2091200) { //error 413
					res.writeHead(413,{'Content-type':'text/html'});
					res.end('<h3>Error: The file exceeds size limits</h3>',
					() => req.connection.destroy()); 
					// callback destroy once res.end returns
				}
			});

			req.on('end', () => {
				let params = qs.parse(body);
				console.log('Username: ',params['username']);
				console.log('Password: ',params['password']);
				res.end();
			});
		}			
	},
	'NA': (req,res) => {
		res.writeHead(404);
		res.end('Content not found!');
	}
}


function router(req,res) {
	// base* are key value pairs 
	// key such as pathname and query are important
	// the second arg in parse 
	//if true - return the query params as key-val pairs
	//if false - returns the query params as a string after '?'
	let baseURI = url.parse(req.url,true);
	
	// index into the function using method and pathname
	let resolveRoute = routes[req.method][baseURI.pathname];

	if (resolveRoute != undefined) {
		req.queryParams = baseURI.query;
		resolveRoute(req,res);
	} else {
		routes['NA'](req,res);
	}
	//let baseURIf = url.parse(req.url,false);
	//console.log("Requested True Route: ",baseURI);
	//console.log("Requested False Route:",baseURIf);
	
	
}


http.createServer(router).listen(3000,() => {
	console.log("Srver running on port 3000")
})
