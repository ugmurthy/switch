$(document).ready(function(){
    var swtxt = "OFF";
    var swlink="/switch"
    var sw = 0;
    $("#switch").text(swtxt);
    $("#switch").click(function(){
        // flip switch
        sw = sw ? 0 : 1
        swtxt = sw ? 'ON' : 'OFF'
        // update button label
        $("#switch").text(swtxt);
        $.post("/switch",{swval:sw});
    });
});
</script>
